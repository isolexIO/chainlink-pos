{
  "expo": {
    "name": "ChainLINK POS",
    "slug": "chainlink-pos",
    "scheme": "chainlinkpos",
    "version": "remote",
    "platforms": ["android"],
    "android": {
      "package": "com.chainlinkpos.app",
      "permissions": [
        "INTERNET",
        "CAMERA"
      ],
      "softwareKeyboardLayoutMode": "adjustResize"
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2a0c784f-43f2-4854-806a-5d49027e486c"
      }
    }
  }
}
